<?xml version="1.0" encoding="utf-8"?>
<TabbedPage 
		xmlns="http://xamarin.com/schemas/2014/forms" 
		xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
		xmlns:pages="clr-namespace:TekConf.Pages;assembly=TekConf" 
		x:Class="TekConf.Pages.MainTabPage" 
		Title="Tabs">

	<NavigationPage Title="Conferences" Icon="conferences.png">
		<x:Arguments>
			<pages:ConferencesListPage />
		</x:Arguments>
	</NavigationPage>
	<NavigationPage Title="My Conferences" Icon="myConferences.png">
		<x:Arguments>
			<pages:MyConferencesPage />
		</x:Arguments>
	</NavigationPage>
	<NavigationPage Title="My Profile" Icon="myProfile.png">
		<x:Arguments>
			<pages:LoginPage Title="Login" />
		</x:Arguments>
	</NavigationPage>
</TabbedPage>